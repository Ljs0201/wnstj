
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// 구구단 만들기
		Scanner sc = new Scanner(System.in);
		System.out.println("몇단 보고 싶은데?");
		
		int 몇단 = sc.nextInt();
		
		int 곱 = 1;
		while(곱<=9) {	
		System.out.println(몇단 + "*" +곱+" = "+(몇단*곱));
		곱++;
		}
		
		}

}
