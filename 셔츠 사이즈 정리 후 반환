        "cnt = [0,0,0,0,0,0]\n",
        "def solution(shirt_size):\n",
        " for i in shirt_size:\n",
        "   if i == \"XS\":\n",
        "    cnt[0]+=1\n",
        "   elif i == \"S\":\n",
        "    cnt[1]+=1\n",
        "   elif i == \"M\":\n",
        "    cnt[2]+=1\n",
        "   elif i == \"L\":\n",
        "    cnt[3]+=1\n",
        "   elif i == \"XL\":\n",
        "    cnt[4]+=1\n",
        "   elif i == \"XXL\":\n",
        "    cnt[5]+=1 \n",
        " return cnt\n",
        "shirt_size = [\"XS\",\"XS\",\"S\",\"M\",\"L\",\"XL\",\"XXL\"]\n",
        "ret = solution(shirt_size);\n",
        "\n",
        "print(\"solution 함수의 반환값은\",ret,\"입니다\")"
      ]
