      "source": [
        "#간단한 사칙연산 계산기 만들기\n",
        "\n",
        "a = int(input(\"연산할 갑\")) #연산할 갑 입력받기 \n",
        "b = input(\"연산자\") #연산자 입력받기\n",
        "c = int(input(\"연산할 갑\")) # 연산할 갑 입력받기\n",
        "\n",
        "if (b == \"+\") : #입력받은 연산자가 +이면\n",
        "  print(a, \"+\", c, \"=\", a+c)  #a+c의 갑 출력\n",
        "elif (b == \"-\"): #입력받은 연산자가 -이면 a-c의 갑 출력\n",
        "  print(a, \"-\", c, \"=\", a-c) #a-c의 갑 출력\n",
        "elif (b == \"*\"): #입력받은 연산자가 *이면 a*c의 갑 출력\n",
        "  print(a, \"*\", c, \"=\", a*c) #a*c의 갑 출력\n",
        "elif (b == \"/\"): #입력받은 연산자가 /이면 a/c의 갑 출력\n",
        "  print(a, \"/\", c, \"=\", a/c) #a/c의 갑 출력\n",
        "elif (b == \"**\"): #입력받은 연산자가 **이면\n",
        "  print(a, \"**\", c, \"=\", a**c) #a**c의 갑 출력\n",
        "elif (b == \"//\"): #입력받은 연산자가 //이면 \n",
        "    print(a, \"//\", c, \"=\", a//c) #a//c의 갑 출력\n",
        "elif (b == \"%\"): #입력받은 연산자가 %이면\n",
        "  print(a, \"%\", c, \"=\", a%c) #a%c의 갑 출력"
      ]
    }
  ]
